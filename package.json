{
  "name": "recruitment-task",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.5.6",
    "graphql": "^16.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dotenv": "^0.1.3",
    "react-scripts": "4.0.3",
    "react-table": "7.7.0",
    "react-use": "17.3.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.0",
    "@octokit/graphql-schema": "^10.73.0",
