      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "react-dotenv": {
    "whitelist": [
      "GITHUB_ACCESS_TOKEN"
